echo
echo "  C compiler               : ${CC}"
echo "  C++ compiler             : ${CXX}"
echo "  Fortran compiler         : ${F77}"
echo "  Python                   : ${HAVE_PYTHON}"
echo
echo "  Timer                    : ${TIMER_NAME}"
echo "  Stack Unwinding          : ${UNWIND_WITH}"
echo "  Address to Source Lookup : ${SOURCE_LOOKUP}"
