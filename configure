echo "  MPI-I/O support          : ${ENABLE_MPI_IO}"
echo "  MPI-RMA support          : ${ENABLE_MPI_RMA}"
echo "  MPI-NBC support          : ${ENABLE_MPI_NONBLOCKINGCOLLECTIVES}"
echo "********************************************************************************"

